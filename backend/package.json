  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "docker-start": "docker-compose up -d && docker logs -f backend",
    "docker-stop": "docker-compose down",
    "jest": "node node_modules/.bin/jest --runInBand --no-cache --watch",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "yarn build:types && yarn build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx,.js\" --source-maps inline --copy-files"
  },
  "engines": {
    "node": ">=12.18.0"
  },
  "keywords": [],
  "dependencies": {
    "@typegoose/typegoose": "^6.1.8",
    "activedirectory": "^0.7.2",
    "async": "^3.1.0",
    "axios": "^0.19.0",
    "connect-history-api-fallback": "^1.6.0",
    "cron": "^1.8.2",
    "email-domain-check": "^1.1.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.7",
    "nanoid": "^3.1.10",
    "nodemailer": "^6.4.5",
    "password-hash-and-salt": "^0.1.4",
    "reflect-metadata": "^0.1.13",
    "serve-static": "^1.14.1",
    "socket.io": "^2.2.0",
